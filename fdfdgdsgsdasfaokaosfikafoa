getgenv().Clipsense = { 
    Aimbot = {
        -- // Defines if the aimbot is enabled.
        
        Enabled = true,
        
        -- // The key for the aimbot.
        
        Key =   "Q",
        
        -- // The hitbox ( wont work if you have UseClosestPart enabled )
        
        Part    = "HumanoidRootPart",
        
        -- // Defines if you wanna lock onto the closest part to your cursor.
        
        UseClosestPart = true,
        
        -- // Defines if the target dies you automatically unlock from the target.
        
	    DisableOnTargetDeath = true,
	    
	    -- // Defines if you die you automatically unlock from the target.
	    
	    DisableOnLocalDeath  = true,
	    
	    -- // If enabled you can only lock onto bots inside of the FOV.
        
        UseFOV      = false,
        
        -- // If the bot leaves your fov, it'll automatically unlock.
        
        MustBeInFOV = false,
        
        -- // Defines if you want the camera to shake when your locked onto a target.
        
	    UseShake   = false,
	    
	    -- // The shaker power, 1-100
	    
	    ShakePower = 100,
	    
	    -- // Defines if you wanna use prediction, ( recommended to keep on )

	    UsePrediction    = true,
	    
	    -- // The prediction amount ( goes according to ping )
	    
        PredictionAmount = 0.146,
        
        -- // If the target is not visible to you, you cannot lock onto them.
        
        WallCheck  = true,
        
        -- // If the target is already dead, you cannot lock onto them anymore.
        
        CheckIfRagdolled = false,
        
        -- // The smoothness ( lower the more legit and the higher the more blatant )

        Smoothness = 0.0986856,
    },
    AimbotFOV = {
        -- // Defines if the FOV is visible.
        Visible = false,
        
        -- // The size of the FOV.
        
        Radius  = 55,
    },
    AutoFOV =  {
        -- // Defines if it is enabled, basically if you equip a gun the fov auto changes.
        Enabled = false,
        
        -- // The FOV for DB
        
        ["Double-Barrel SG"] = {["FOV"] = 15},
        
        -- // The FOV for Revolver
        
        ["Revolver"] = {["FOV"] = 10},
        
        -- // The FOV for SMG
        
        ["SMG"] = {["FOV"] = 23},
        
        -- // The FOV for Shotgun
        
        ["Shotgun"] = { ["FOV"] = 20},
        
        -- // The FOV for Tactical Shotgun
        
        ["TacticalShotgun"] = {["FOV"] = 24},
	},
}

-- // Loading the script . . .

loadstring(game:HttpGet("https://raw.githubusercontent.com/lucidVVS/ClipSense/main/Main"))()
